pipeline{
    agent any
    stages{
        stage('Build'){
            steps{
                sh 'cd coit-frontend && npm install && npm run build'
            }
        }
        stage('Build Docker Image') {  
            steps{                     
	             sh 'sudo docker build -t <vijayakurra>/<coit-frontend>:latest .'     
	             echo 'Build Image Completed'                
              } 
           }
    }
}
