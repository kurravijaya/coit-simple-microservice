pipeline {
    agent {
    // Equivalent to "docker build -f Dockerfile.build --build-arg version=1.0.2 ./build/
    dockerfile {
        filename 'Dockerfile-multistage.build'
        dir 'coit-frontend/Dockerfile-multistage'
        
    }
}
stages{
   stage('Build'){
      steps{
	      sh echo 'build in jenkinfile'
}
}
}
}
